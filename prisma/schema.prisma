
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode  = "prisma"
}
generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int     @id  @default(autoincrement())
  email   String   @unique
  name    String?
  role String
  password String @db.Char(60)
}